<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ShapeAppearanceOverlay.Rectangle">
        <item name="cornerFamily">cut</item> <item name="cornerSize">0dp</item> </style>

    <style name="KeyLab3BaseSquare" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_columnWeight">1</item>
        <item name="android:layout_rowWeight">1</item>
        <item name="android:textSize">30sp</item>
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_margin">1dp</item> <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.Rectangle</item>
        <item name="android:insetTop">0dp</item> <item name="android:insetBottom">0dp</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
    </style>

    <style name="KeyLab3Number" parent="KeyLab3BaseSquare">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:backgroundTint">#616161</item> </style>

    <style name="KeyLab3Operator" parent="KeyLab3BaseSquare">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:backgroundTint">#333333</item> </style>

    <style name="KeyLab3Delete" parent="Widget.MaterialComponents.Button">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textSize">20sp</item> <item name="android:textColor">#FFFFFF</item>
        <item name="android:gravity">center</item>
        <item name="android:backgroundTint">#333333</item> <item name="android:layout_margin">1dp</item> <item name="shapeAppearanceOverlay">@style/ShapeAppearanceOverlay.Rectangle</item>
        <item name="android:insetTop">0dp</item> <item name="android:insetBottom">0dp</item>
        <item name="android:insetLeft">0dp</item>
        <item name="android:insetRight">0dp</item>
    </style>

    <style name="Button_Lab3">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">20sp</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:backgroundTint">#A5A5A5</item>
    </style>

    <style name="KeyBase">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_columnWeight">1</item>
        <item name="android:layout_rowWeight">1</item>
        <item name="android:layout_margin">4dp</item>
        <item name="android:textSize">28sp</item>
        <item name="android:backgroundTint">#555555</item>
    </style>

    <style name="KeyGray" parent="KeyBase">
        <item name="android:textColor">#000000</item>
        <item name="android:backgroundTint">#A5A5A5</item>
    </style>

    <style name="KeyDark" parent="KeyBase">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:backgroundTint">#333333</item>
    </style>

    <style name="KeyOrange" parent="KeyBase">
        <item name="android:textColor">#FFFFFF</item>
        <item name="android:backgroundTint">#FF9500</item>
    </style>

</resources>